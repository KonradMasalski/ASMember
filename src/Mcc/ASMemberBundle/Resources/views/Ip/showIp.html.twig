{# empty Twig template #}
{% extends 'MccASMemberBundle::layout.html.twig' %}
{% block sidebar %}
<table class="record_properties">
    <tbody>
        <tr>
            <th>Range Id</th>
            <td>{{ rangeId }}</td>
        </tr>
        <tr>
            <th>Iprange</th>
            <td>{{ range }}</td>
        </tr>
        <tr>
            <th>Asidentifier</th>
            <td>{{ asidentifier }}</td>
        </tr>
        <tr>
            <th>Asname</th>
            <td>{{ asname }}</td>
        </tr>
    </tbody>
</table>
<ul class="record_actions">
    <li>
    <a onclick="javascript:history.back()" style="text-decoration:underline; color:blue; cursor:hand">Click here to go back</a> 
    </li>
    </ul>
 {% endblock %}

{% block content %}
<h2>Informacje o adresie IP:</h2>

{% if komunikat == 1 %}
<tr>
    <td>
        Adres IP: {{ip}} nie by≈Ç sprawdzany
    </td>
    <td>
        <a href="{{ path('check_ip', { 'ip': ip,'rangeid': rangeId }) }}" class="ajaxable">check</a>
    </td>
</tr>
{% else %}
    <tr>
    <td>
        Adres IP: {{ip}} jest web serwerem, 
    </td>
    <td>
        jego AS to {{ asidentifier }}, 
    </td>
    <td>
       {{ip_addr.getHostname}} 
    </td>
</tr>
    {% endif %}
<script>
    var ajax = "{{ asset('images/ajax-loader.gif') }}";
</script>

    {% endblock %}